#!/home/batisteo/Side/Projects/vscode-django/.venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'vscode-django','console_scripts','syntax'
__requires__ = 'vscode-django'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vscode-django', 'console_scripts', 'syntax')()
    )
