{
  "coc.preferences.bracketEnterImprove": true,
  "coc.source.buffer.priority": 80,
  "coc.source.neco.priority": 100,
  "coc.source.syntax.priority": 90,
  "tabnine.priority": 85,
  "codeLens.enable": true,
  "diagnostic.errorSign": "",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.virtualText": true,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh",
        "bash"
      ]
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": [
        "vim",
        "markdown",
        "yaml"
      ]
    }
  },
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.completion.postfix.enable": true,
  "snippets.priority": 85,
  "snippets.ultisnips.enable": false,
  "suggest.noselect": false,
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": ""
  }
}
